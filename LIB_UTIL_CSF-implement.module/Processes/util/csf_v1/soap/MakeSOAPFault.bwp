                    <tibex:inputBindings>
                        <tibex:inputBinding
                            expression="&lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot;?>&#xa;&lt;xsl:stylesheet xmlns:xsl=&quot;http://www.w3.org/1999/XSL/Transform&quot; xmlns:tns2=&quot;http://www.royallondon.com/xsd/interface/util/csf/soap/v1&quot; xmlns:xsd=&quot;http://www.w3.org/2001/XMLSchema&quot; xmlns:ftl=&quot;http://www.royallondon.com/xsd/resource/util/csf/ftl/v1&quot; xmlns:tib=&quot;http://www.tibco.com/bw/xslt/custom-functions&quot; version=&quot;2.0&quot;>&#xa;    &lt;xsl:param name=&quot;Start&quot;/>&#xa;    &lt;xsl:param name=&quot;Mapper&quot;/>&#xa;    &lt;xsl:param name=&quot;xmlFormat&quot;/>&#xa;    &lt;xsl:param name=&quot;stringFormat&quot;/>&#xa;    &lt;xsl:param name=&quot;csfFormat&quot;/>&#xa;    &lt;xsl:template name=&quot;End-input&quot; match=&quot;/&quot;>&#xa;        &lt;tns2:MakeSOAPFaultResponse>&#xa;            &lt;xsl:choose>&#xa;                &lt;xsl:when test=&quot;$Start/tns2:Format = $xmlFormat&quot;>&#xa;                    &lt;xsl:copy-of select=&quot;$Mapper&quot;/>&#xa;                &lt;/xsl:when>&#xa;                &lt;xsl:when test=&quot;$Start/tns2:Format = $stringFormat&quot;>&#xa;                    &lt;tns2:PayloadString>&#xa;                        &lt;xsl:value-of select=&quot;tib:render-xml($Mapper, true(), false())&quot;/>&#xa;                    &lt;/tns2:PayloadString>&#xa;                &lt;/xsl:when>&#xa;                &lt;xsl:when test=&quot;$Start/tns2:Format = $csfFormat&quot;>&#xa;                    &lt;ftl:CSFMessage>&#xa;                        &lt;ftl:channel>&#xa;                            &lt;xsl:value-of select=&quot;$Start/tns2:Channel&quot;/>&#xa;                        &lt;/ftl:channel>&#xa;                        &lt;ftl:message>&#xa;                            &lt;xsl:value-of select=&quot;xsd:base64Binary(tib:string-to-base64(tib:render-xml($Mapper, true(), false())))&quot;/>&#xa;                        &lt;/ftl:message>&#xa;                    &lt;/ftl:CSFMessage>&#xa;                &lt;/xsl:when>&#xa;                &lt;xsl:otherwise>&#xa;                    &lt;tns2:PayloadBase64Binary>&#xa;                        &lt;xsl:value-of select=&quot;xsd:base64Binary(tib:string-to-base64(tib:render-xml($Mapper, true(), false())))&quot;/>&#xa;                    &lt;/tns2:PayloadBase64Binary>&#xa;                &lt;/xsl:otherwise>&#xa;            &lt;/xsl:choose>&#xa;        &lt;/tns2:MakeSOAPFaultResponse>&#xa;    &lt;/xsl:template>&#xa;&lt;/xsl:stylesheet>" expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xslt1.0"/>
                    </tibex:inputBindings>
